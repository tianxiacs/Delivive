@using Delivive.Models;
@model IEnumerable<FoodModel>

<table>
@foreach (FoodModel item in Model)
{
    <tr>
        <td>@item.Food_name</td>
        <td>@item.Price</td>
        <td>@item.Description</td>
        <td><input type="button" value="Add to Cart" id="@item.Restaurant_id" onclick="makeOrder(@item.Restaurant_id)"></td>
    </tr>
}
</table>


<p id="ShoppingCart">

</p>

